// clang-format on
#pragma once

namespace std {

#if __cplusplus < 202302L

// https://en.cppreference.com/w/cpp/utility/unreachable.html
[[noreturn]] inline void unreachable() {
  __builtin_unreachable();
}

#endif // __cplusplus < 202302L

} // namespace std
